    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "sharp": "^0.33.4",
    "svgo": "^3.3.2",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
