    "input-otp": "^1.2.4",
    "lucide-react": "^0.379.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "styled-components": "^6.1.11",
    "tailwind-merge": "^2.3.0",
